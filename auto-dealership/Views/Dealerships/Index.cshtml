@using Dealerships.Models;

@if (Model.Count == 0)
{
    <p>No Cars match your criteria</p>
}
<ul>        
     @foreach (Car automobile in Model)
            {
                <li>@automobile.MakeModel;</li>
                <li>@automobile.Price;</li>
                <li>@automobile.Miles;</li>

            }
</ul>